{
    "name": "yyfjj/laravel-apidoc-generator",
    "license": "MIT",
    "description": "Generate beautiful API documentation from your Laravel application",
    "keywords": [
        "API",
        "Documentation",
        "Laravel"
    ],
    "homepage": "http://github.com/yyfjj/laravel-apidoc-generator",
    "authors": [
        {
            "name": "benjamin",
            "email": "yyfjj@foxmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.5.* || 3.6.* || 3.7.*",
        "phpunit/phpunit": "^6.0.0 || ^7.4.0",
        "dingo/api": "2.0.0-alpha1",
        "mockery/mockery": "^1.2.0",
        "league/fractal": "^0.17.0"
    },
    "suggest": {
        "league/fractal": "Required for transformers support",
        "nunomaduro/collision": "For better reporting of errors that are thrpwn when generating docs"
    },
    "autoload": {
        "psr-4": {
            "Mpociot\\ApiDoc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mpociot\\ApiDoc\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test-ci": "phpunit --coverage-clover=coverage.xml"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mpociot\\ApiDoc\\ApiDocGeneratorServiceProvider"
            ]
       }
    }
}